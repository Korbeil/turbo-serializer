{
    "name": "korbeil/turbo-serializer",
    "type": "library",
    "licence": "MIT",
    "authors": [
        {"name": "Mathias Arlaud", "email": "mathias.arlaud@gmail.com"},
        {"name": "Baptiste Leduc", "email": "baptiste.leduc@gmail.com"}
    ],
    "require": {
        "php": ">=8.2",
        "jolicode/automapper-bundle": "^8.0",
        "mtarld/json-encoder-bundle": "^0.1",
        "symfony/serializer": "^7.1@dev",
        "symfony/type-info": "^7.1@dev",
        "symfony/var-exporter": "^7.1@dev",
        "phpstan/phpdoc-parser": "^1.26"
    },
    "autoload": {
        "psr-4": { "TurboSerializer\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "TurboSerializer\\Tests\\": "tests/" },
        "classmap": ["tests/App/AppKernel.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "symfony/http-kernel": "^7.0",
        "symfony/framework-bundle": "^7.1@dev",
        "symfony/filesystem": "^7.1@dev",
        "symfony/yaml": "^7.0"
    }
}
